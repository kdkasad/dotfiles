text/plain; less -F %s ; copiousoutput;
text/rfc822-headers; less -F %s ; copiousoutput;
text/html; lynx -assume_charset=%{charset} -display_charset=utf-8 -dump %s; nametemplate=%s.html; copiousoutput;
audio/*; setsid -f xdg-open %s ;
image/*; setsid -f xdg-open %s ;
video/*; setsid -f xdg-open %s ;
application/pdf; setsid -f xdg-open %s ;
application/pgp-encrypted; gpg -d '%s'; copiousoutput;
application/pgp-keys; gpg --import '%s'; copiousoutput;
application/*; setsid -f xdg-open %s ;
